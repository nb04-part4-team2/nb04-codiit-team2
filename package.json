{
  "name": "codiit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:integration": "dotenv -e .env.test -e .env.test.local -- node --experimental-vm-modules node_modules/.bin/jest --config jest.integration.config.cjs --runInBand --passWithNoTests",
    "test:all": "npm test && npm run test:integration",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d --wait",
    "test:db:down": "docker compose -f docker-compose.test.yml down",
    "test:db:reset": "docker compose -f docker-compose.test.yml down -v && docker compose -f docker-compose.test.yml up -d --wait && dotenv -e .env.test -- prisma migrate deploy",
    "test:db:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/server.js",
    "lint": "eslint src __tests__ --ext .ts",
    "lint:fix": "eslint src __tests__ --ext .ts --fix",
    "format": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "check": "npm run lint && npm run format",
    "fix": "npm run lint:fix && npm run format:fix",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit",
    "typecheck:all": "npm run typecheck && npm run typecheck:test",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --runInBand",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "lint-staged": {
    "**/*.ts": [
      "npx eslint --fix",
      "npx prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "babel-jest": "^30.2.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.13"
  }
}
